	<div id="contact" class="content-section-a" ng-app="validationApp" ng-controller="mainController">
		<div class="container">
			<div class="row">

			<div class="col-md-6 col-md-offset-3 text-center wrap_title">
				<h2>Contact Us</h2>
				<p class="lead" style="margin-top:0">We want to talk with you</p>
			</div>

			<form name="userForm" ng-submit="submitForm()" novalidate >
				<div class="col-md-6">
					<div class="form-group" ng-class="{ 'has-error' : userForm.name.$invalid && !userForm.name.$pristine }">
						<label for="name">Your Name</label>
						<div class="input-group">
							<input type="text" class="form-control" name="name" id="name" placeholder="Enter Name" required  ng-model="user.name" title="We ask for your name" >
							<span class="input-group-addon"><i class="ion-person"></i></span>
							
						</div>
						<p ng-show="userForm.name.$invalid && !userForm.name.$pristine" class="help-block">You name is required.</p>
					</div>

					<div class="form-group" ng-class="{ 'has-error' : userForm.email.$invalid && !userForm.email.$pristine }">
						<label for="InputEmail">Your Email</label>
						<div class="input-group">
							<input type="email" class="form-control" id="InputEmail" name="email" placeholder="Enter Email" required ng-model="user.email" title="We ask for you email">
							<span class="input-group-addon"><i class="ion-email"></i></span>
						</div>
						<p ng-show="userForm.email.$invalid && !userForm.email.$pristine" class="help-block">Enter a valid email.</p>
					</div>
					
					
                    <div class="form-group">
						<label for="date">Date</label>
						<div class="input-group">
						<input type="date" value="10/24/1984" class="form-control" id="date" name="date" placeholder="Enter date" required  title="We ask for you email"/>
							
							<span class="input-group-addon"><i class="ion-calendar"></i></span>
						</div>
						
					</div>

					<div class="form-group">
						<label for="InputMessage">Message</label>
						<div class="input-group">
							<textarea name="InputMessage" id="InputMessage" class="form-control" rows="5" required title="We ask for your message "></textarea>
							<span class="input-group-addon"><i class="ion-android-chat"></i></span>
						</div>
					</div>

                    <input type="submit" name="submit" id="submit" value="Submit" class="btn wow tada btn-embossed btn-primary pull-right" ng-disabled="userForm.$invalid">
				</div>
			</form>

			<hr class="featurette-divider hidden-lg">
